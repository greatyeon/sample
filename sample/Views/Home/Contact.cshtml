@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>
<form id="TestParameter" name="TestParameter">


    <table id="valueTable" style="border: 1px solid black; width: 500px; text-align: center">
        <thead>
        <tr>
            <td>구분</td>
            <td>값 입력</td>
            <td>결과 값</td>
        </tr>
        </thead>
        <tr>
            <td>아이디</td>
            <td><input type="number" name="Id" maxlength="10"/></td>
            @*<td><input id="return_id" disabled="true"/></td>*@
            <td><p id="return_id"></p></td>
        </tr>
        <tr>
            <td>이 름</td>
            <td><input name="Name" maxlength="10"/></td>
            <td><p id="return_name"></p></td>
        </tr>
        <tr>
            <td>이메일</td>
            <td><input name="Email" maxlength="10"/></td>
            <td><p id="return_email"></p></td>
        </tr>
        <tr>
            <td>주 소</td>
            <td><input name="Address" maxlength="10"/></td>
            <td><p id="return_address"></p></td>
        </tr>
        <tr>
            <td>전화번호</td>
            <td><input name="Phone" maxlength="10"/></td>
            <td><p id="return_phone"></p></td>
        </tr>
        <tr>
            <td>날 짜</td>
            <td colspan="2"><p id="return_nowdate"></p></td>       
        </tr>
       

    </table>
    <input type="button" id="btnOk" value="값 입력" />
</form>

<address>
    <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>